{literal}
<form name="frm-contatoscrea" method="post">
    <section class="form-group">
        <label for="id" class="upper">id:</label>
        <input type="text" ng-model="contatoscre.id" class="form-control" id="id" autocomplete="off"  />
    </section>
    <section class="form-group">
        <label for="nome" class="upper">nome:</label>
        <input type="text" ng-model="contatoscre.nome" class="form-control" id="nome" autocomplete="off" maxlength="90" />
    </section>
    <section class="form-group">
        <label for="crea" class="upper">crea:</label>
        <input type="text" ng-model="contatoscre.crea" class="form-control" id="crea" autocomplete="off" maxlength="2" />
    </section>
    <section class="form-group">
        <label for="departamento" class="upper">departamento:</label>
        <input type="text" ng-model="contatoscre.departamento" class="form-control" id="departamento" autocomplete="off" maxlength="45" />
    </section>
    <section class="form-group">
        <label for="email" class="upper">email:</label>
        <input type="text" ng-model="contatoscre.email" class="form-control" id="email" autocomplete="off" maxlength="90" />
    </section>
    <section class="form-group">
        <label for="telefone" class="upper">telefone:</label>
        <input type="text" ng-model="contatoscre.telefone" class="form-control" id="telefone" autocomplete="off"  />
    </section>
    <section class="form-group">
        <label for="endereco" class="upper">endereco:</label>
        <input type="text" ng-model="contatoscre.endereco" class="form-control" id="endereco" autocomplete="off" maxlength="90" />
    </section>
    <section class="form-group">
        <label for="bairro" class="upper">bairro:</label>
        <input type="text" ng-model="contatoscre.bairro" class="form-control" id="bairro" autocomplete="off" maxlength="45" />
    </section>
    <section class="form-group">
        <label for="cep" class="upper">cep:</label>
        <input type="text" ng-model="contatoscre.cep" class="form-control" id="cep" autocomplete="off"  />
    </section>
    <section class="form-group">
        <label for="cidade" class="upper">cidade:</label>
        <input type="text" ng-model="contatoscre.cidade" class="form-control" id="cidade" autocomplete="off" maxlength="45" />
    </section>
    <section class="form-group">
        <label for="criado_por" class="upper">criado por:</label>
        <input type="text" ng-model="contatoscre.criado_por" class="form-control" id="criado_por" autocomplete="off" maxlength="45" />
    </section>
    <section class="form-group">
        <label for="alterado_por" class="upper">alterado por:</label>
        <input type="text" ng-model="contatoscre.alterado_por" class="form-control" id="alterado_por" autocomplete="off" maxlength="45" />
    </section>
</form>
{/literal}
